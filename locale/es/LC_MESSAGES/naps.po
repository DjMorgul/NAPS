#: edita_bd_pics.py:167
#, python-format
msgid ""
"The chosen image (%s) has invalid dimensions, both its width and its height "
"should be bigger than zero"
msgstr ""
"La imagen elegida (%s) tiene dimensiones inválidas, tanto su ancho como su "
"alto debería ser mayor que cero"

#: edita_bd_pics.py:170
msgid "Excessive image height"
msgstr "Altura de imagen excesiva"

#: edita_bd_pics.py:170
#, python-format
msgid ""
"The chosen image (%(fileName)s) has a height of %(imageHeight)d pixels, "
"while the mode %(graphicMode)s of the graphic database only supports up to "
"%(maxHeight)d"
msgstr ""
"La imagen elegida (%(fileName)s) tiene %(imageHeight)d píxeles de alto, "
"mientras que el modo %(graphicMode)s de la base de datos gráfica sólo "
"soporta hasta %(maxHeight)d"

#: edita_bd_pics.py:173
msgid "Excessive image width"
msgstr "Anchura de imagen excesiva"

#: edita_bd_pics.py:173
#, python-format
msgid ""
"The chosen image (%(fileName)s) has a width of %(imageWidth)d pixels, while "
"the mode %(graphicMode)s of the graphic database only supports up to "
"%(maxWidth)d"
msgstr ""
"La imagen elegida (%(fileName)s) tiene %(imageWidth)d píxeles de ancho, "
"mientras que el modo %(graphicMode)s de la base de datos gráfica sólo "
"soporta hasta %(maxWidth)d"

#: edita_bd_pics.py:176
msgid "Incorrect image height"
msgstr "Altura de imagen incorrecta"

#: edita_bd_pics.py:176
#, python-format
msgid ""
"The chosen image (%(fileName)s) has a height of %(imageHeight)d pixels, when "
"it should be a multiple of 8"
msgstr ""
"La imagen elegida (%(fileName)s) tiene %(imageHeight)d píxeles de alto, "
"cuando debería ser múltiplo de 8"

#: edita_bd_pics.py:179
msgid "Incorrect image width"
msgstr "Anchura de imagen incorrecta"

#: edita_bd_pics.py:179
#, python-format
msgid ""
"The chosen image (%(fileName)s) has a width of %(imageWidth)d pixels, when "
"it should be a multiple of 8"
msgstr ""
"La imagen elegida (%(fileName)s) tiene %(imageWidth)d píxeles de ancho, "
"cuando debería ser múltiplo de 8"

#: edita_bd_pics.py:199
msgid "High number of colors"
msgstr "Número de colores elevado"

#: edita_bd_pics.py:199
#, python-format
msgid ""
"The chosen image (%(fileName)s) uses %(colorCount)d different colors, while "
